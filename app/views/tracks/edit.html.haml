%table{:width => "40%", :align => "center", :id => "recordingTrackTable"}
  %tr
    %th Track
    %th Song(s)
  - @tracks.each do |track|
    %tr
      %td= track.track
      %td 
        - track.setlists.each do |setlist|
          = setlist.song.song_name

%form{:action => "/tracks/add", :method => "post"}
  #addTracks
  %h5 Track Number
  = text_field :name => "track"
  #addTracks
  %h5 Songs on Track
  = select :name => "setlist", :collection => @setlists, :size=>"10", :multiple => true
  %input{:type => "hidden", :name => "recording_id", :value => "#{@recording.id}"}
  #addTracks
  = submit "Add Tracks" 