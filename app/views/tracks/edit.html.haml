= javascript_include_tag "admin"
%br
%table{:width => "50%", :align => "center", :bgcolor => "#FFFFFF"}
  %tr
    %td= render 'shows/setlist'
%br
%table{:width => "90%", :align => "center"}
  %tr
    %th Track Number
    %th Song Start
    %th Song End
    %th Save
  - @recording_tracks.each do |recording_track|
    %form{:action => '/tracks/add', :method => 'post'}
      %tr
        %td= recording_track.track
        %td
          %input{:type => "text", :name => "song_id_start", :id => "song_id_start", :size => "25", :value => recording_track.song_name_start}
        %td
          %input{:type => "text", :name => "song_id_start", :id => "song_id_end", :size => "25", :value => recording_track.song_name_end}
        %td
          %input{:type => "submit", :value => "Update"}
      
    